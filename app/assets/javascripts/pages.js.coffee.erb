change_time = ->
    date = Date.parse("<%= Time.zone.now %>")
    d = $("#time_delimiter")
    h = $("#hour")
    m = $("#minute")
    d.toggleClass("white-delimiter")
    h.html date.getHours()
    m.html "#{(if date.getMinutes() < 10 then '0' else '')}#{date.getMinutes()}"

    
jQuery ->
    setInterval change_time, 1000

